{
  "name": "music-bot",
  "version": "1.0.0",
  "main": "app.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "dev": "yarn run build:live",
    "start": "yarn run build && node dist/app.js",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "typescript-collections": "^1.3.3",
    "url-regex": "^5.0.0",
    "ytdl-core": "^3.1.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.11",
    "@types/ws": "^7.2.5",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
