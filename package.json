{
  "name": "music-bot",
  "version": "1.0.0",
  "main": "app.ts",
  "license": "MIT",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "dev": "npm run build:live",
    "start": "npm run build && node dist/app.js",
    "build": "tsc -p .",
    "clean": "rm -r dist/ && rm -r node_modules/ && rm package-lock.json",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "discord.js": "^12.4.0",
    "dotenv": "^8.2.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "typescript-collections": "^1.3.3",
    "url-regex": "^5.0.0",
    "yt-search": "^2.3.3",
    "ytdl-core-discord": "^1.2.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/ws": "^7.2.7",
    "@types/yt-search": "^2.3.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4.0.3"
  }
}
